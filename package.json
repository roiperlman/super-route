    "@types/cookie-parser": "^1.4.2",
    "@types/ejs": "^2.6.3",
    "@types/mocha": "^8.2.0",
    "@types/morgan": "^1.9.2",
    "chai-as-promised": "^7.1.1",
    "cookie-parser": "^1.4.4",
    "ejs-lint": "^1.2.0",
